    <script src="~/Content/CustomHelper.js"></script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // The DOM is fully loaded, hide the loader
            document.getElementById("loader-id").style.display = "none";
        });

        // If you also want to handle external resources like images, use the load event
        window.addEventListener("load", function () {
            // The entire page, including external resources, is fully loaded, hide the loader
            document.getElementById("loader-id").style.display = "none";
        });
        $(document).ready(function () {
            $('.owl-carousel').owlCarousel({
                margin: 10,
                loop: false,
                autoWidth: true,
                items: 4,
                dots: false,
                touchDrag: false,
                rtl: true

            });
            localStorage.clear();
        })</script>
    <script>
        function ActivateLoaderFunction() {
            var myDiv = document.getElementById("loader-id");
            myDiv.style.display = "flex";
        }
        function DeactivateLoaderFunction() {
            var myDiv = document.getElementById("loader-id");
            myDiv.style.display = "none";
        }</script>
    <script>
        const shareButton = document.querySelector("#shareButton");
        var shareableLink = window.location.href;
        shareButton.addEventListener("click", (event) => {
            if (navigator.share) {
                navigator.share({
                    title: "Share club details",
                    url: shareableLink,
                }).then(() => {
                    console.log('Thanks for sharing!');
                }).catch(console.error);
            } else {
                toastr.info('Web Share API is not supported in this browser.');
            }
        });</script>
    <script>
        function OpenImagePopup(ImageURL) {
            if (ImageURL != '' && ImageURL != null) {
                var modalImg = document.getElementById("img01");
                var modal = document.getElementById("myModal");
                modal.style.display = "flex";
                modalImg.src = ImageURL;
            }
        }

        function closeImagePopup() {
            var modal = document.getElementById("myModal");
            modal.style.display = "none";
        }</script>

    <script>
        function openModal() {
            console.log("test");
        }</script>
    <script>
        // Function to open a specific modal
        function openModal(modalId) {
            var modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = "flex";
            }
        }
        // Function to close a specific modal
        function closeModal(modalId) {
            var modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = "none";
            }
        }
        // Close the modal if the overlay is clicked
        window.onclick = function (event) {
            var modals = document.getElementsByClassName("modal");
            for (var i = 0; i < modals.length; i++) {
                if (event.target == modals[i]) {
                    closeModal(modals[i].id);
                }
            }
        }
        // Close the modal only if the overlay is clicked outside of modal1
        window.onclick = function (event) {
            var modal1 = document.getElementById('modal1');
            // Check if the click is outside of modal1
            if (modal1 && event.target !== modal1 && !modal1.contains(event.target)) {
                closeModal('modal1');
            }
        }</script>

    <script>
$(document).ready(function () {
            $("#scheduleDropdown").change(function () {
                localStorage.setItem('SelectedDate', $(this).val());
                var selectedValue = localStorage.getItem('SelectedDate');
                $.ajax({
                    url: '/LocationManagement/ClubDetail_V2?ScheduleFilterDate=' + selectedValue + '&ClubId=' + '@ViewBag.ClubId' + '&LocationId=' + '@ViewBag.LocationId',
                    type: 'POST',
                    data: { ScheduleFilterDate: selectedValue },
                    success: function (result) {
                        window.location.href = '/LocationManagement/ClubDetail_V2?ScheduleFilterDate=' + selectedValue + '&ClubId=' + '@ViewBag.ClubId' + '&LocationId=' + '@ViewBag.LocationId';
                    },
                    error: function (error) {
                        window.location.href = '/LocationManagement/ClubDetail_V2?ScheduleFilterDate=' + selectedValue + '&ClubId=' + '@ViewBag.ClubId' + '&LocationId=' + '@ViewBag.LocationId';
                    }
                });
            });
        });</script>
    <script>
        $('#notice_id').click('click', function () {
            var activeElement = document.querySelector('.tab-link.active');
            if (activeElement) {
                activeElement.classList.remove('active');
            }
            var newActiveElement = document.getElementById('tab6Button');
            if (newActiveElement) {
                newActiveElement.classList.add('active');
                $('#tab6Button').click();
            }
        });
        $('#schedule_id').click('click', function () {
            var activeElement = document.querySelector('.tab-link.active');
            if (activeElement) {
                activeElement.classList.remove('active');
            }
            var newActiveElement = document.getElementById('tab6Button');
            if (newActiveElement) {
                newActiveElement.classList.add('active');
                $('#tab5Button').click();
            }
        });</script>

    <!--make clicked the tab after reload-->
    <script>
        $('.tab-button-plane').click('click', function () {
            var id = $(this).attr('id');
            var name = $(this).text().trim();
            localStorage.setItem("tablink-id", id);
            localStorage.setItem("tablink-name", name);
        });
        $(document).ready(function () {
            var id = localStorage.getItem("tablink-id");
            var name = localStorage.getItem("tablink-name");
            $('.tab-button-plane').each(function (index) {
                var valueId = $(this).attr('id').trim();
                var valueName = $(this).text().trim();
                if (id == valueId && name == valueName) {
                    $(this).click();
                }
            });
        });</script>

    <!--comment div clicked event-->
    <script>
        $('#comment_ID').click('click', function () {
            $('#tab3Button').click();
        });</script>
