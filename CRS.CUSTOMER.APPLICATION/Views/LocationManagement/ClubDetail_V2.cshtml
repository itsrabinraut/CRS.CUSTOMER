                                                    <line x1="9.5" y1="15" x2="9.5" y2="31" stroke="#C9CACA" />
                                                </svg>
                                            }
                                            else if (!string.IsNullOrEmpty(item.Rank) && item.Rank.Trim() == "3")
                                            {
                                                <svg xmlns="http://www.w3.org/2000/svg" width="19" height="31"
