@using CRS.CUSTOMER.APPLICATION.Library
@model CRS.CUSTOMER.APPLICATION.Models.LocationManagement.ClubDetailModel
@{
    ViewBag.Title = "Club Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
    DateTime todayDate = DateTime.Now;
    string formattedDate = todayDate.ToString("yyyy年M月");

}
<script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.8.1/flowbite.min.js"></script>
<script src="/Content/assets/js/tabs.js"></script>
<style>
    .stickyBox {
        position: -webkit-sticky;
        position: sticky;
        bottom: 0;
        height: 30px;
        width: 100vw;
        background: yellow;
    }

    .tab-container-plane .active {
        border-bottom: 2px solid #E3A1A1;
    }
</style>
<!-- breadcrumb  -->
<div class="flex py-[8px] px-4 gap-1 items-center breadcrumb">
    <a href="@Url.Action("Index", "Dashboard")" class="active">
        @CRS.CUSTOMER.APPLICATION.Resources.Resource.Home
    </a>
    <i class="las la-angle-right"></i>
    <a href="@Url.Action("Index", "LocationManagement", new { LocationId = Model.LocationId })" class="active">
        @CRS.CUSTOMER.APPLICATION.Resources.Resource.Clubs
    </a>
    <i class="las la-angle-right"></i>
    <a href="#">@Model.ClubNameEng</a>
</div>
<div class="h-[189px]">
    <img src="@Model.ClubCoverPhoto" alt="banner" class="h-[189px] w-full object-cover"
         style="height: 189px;" />

</div>
<!-- section 1 -->
<div class="px-2 pt-[12px] pb-2 flex gap-1">
    @if (!string.IsNullOrEmpty(Model.Tag1))
    {

        <div class="px-[10px] border-[1px] border-[#A09E91] text-[10px] rounded-[2px] text-[#A09E91] flex justify-start items-center self-start gap-[2px]" style="border-radius: 2px; border:1px solid #F6F6F6">
            <svg xmlns="http://www.w3.org/2000/svg" width="11" height="10" viewBox="0 0 11 10" fill="none">
                <path fill-rule="evenodd" clip-rule="evenodd"
                      d="M2.6875 3.75C2.6875 2.20093 3.95093 0.9375 5.5 0.9375C7.04907 0.9375 8.3125 2.20093 8.3125 3.75C8.3125 4.18945 8.13428 4.6936 7.89258 5.24414C7.65088 5.79468 7.3396 6.37939 7.02344 6.92383C6.39111 8.01392 5.75391 8.92578 5.75391 8.92578L5.5 9.29688L5.24609 8.92578C5.24609 8.92578 4.60889 8.01392 3.97656 6.92383C3.6604 6.37939 3.34912 5.79468 3.10742 5.24414C2.86572 4.6936 2.6875 4.18945 2.6875 3.75ZM5.5 3.125C5.15454 3.125 4.875 3.40454 4.875 3.75C4.875 4.09546 5.15454 4.375 5.5 4.375C5.84546 4.375 6.125 4.09546 6.125 3.75C6.125 3.40454 5.84546 3.125 5.5 3.125Z"
                      fill="#D75A8B" />
            </svg>
            <span>@Model.Tag1</span>
        </div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag2))
    {
        <div class="px-[5px] py-[1px] bg-[#BFA3B6] text-[10px] rounded-[2px] text-white">@Model.Tag2</div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag3))
    {
        <div class="px-[5px] py-[1px] bg-[#697DBF] text-[10px] rounded-[2px] text-white">@Model.Tag3</div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag4))
    {
        <div class="px-[5px] py-[1px] bg-[#96B41F] text-[10px] rounded-[2px] text-white">@Model.Tag4</div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag5))
    {
        <div class="px-[5px] py-[1px] bg-[#EFB726] text-[10px] rounded-[2px] text-white">@Model.Tag5</div>
    }
</div>
<!-- section 2 -->
<div class="px-4 pb-[8px] flex justify-between ">
    <div class="flex gap-[8px]">
        @if (Model.ClubLogo != null || Model.ClubLogo != "")
        {
            <div data-modal-target="" data-modal-toggle="">
                <img src="@Model.ClubLogo"
                     class="h-[48px] w-[48px] object-cover rounded-full border-[1px] border-[#FAFAFA]" />
            </div>
        }
        else
        {
            <div data-modal-target="" data-modal-toggle="">
                <img src="/Content/assets/images/logo.svg"
                     class="h-[48px] w-[48px] object-cover rounded-full border-[1px] border-[#FAFAFA]" />
            </div>
        }
        <div>
            <div class='text-sm font-bold text-charcolBlack '>@Model.ClubNameEng</div>
            <div class='text-xs  text-[#404040] '>@Model.ClubNameJp</div>
            <div class="text-[10px] text-[#E3A1A1]">@Model.GroupName</div>
        </div>
    </div>
    @if (Model.IsBookmarked.Trim().ToUpper() == "N")
    {
        <div class="flex items-center flex-col" onclick="ManageBookmark('@Model.ClubId', '', 'club', this)">
            <div class="p-1">
                <img src="~/Content/assets/images/customer/bookmark-outlined.svg" class="h-[20px] w-[25px]" />
            </div>
            <span class="text-[#BCB08A] text-[9px] font-medium" style="line-height: 100%;">保存</span>
        </div>
    }
    else
    {
        <div class="flex items-center flex-col" onclick="ManageBookmark('@Model.ClubId', '', 'club', this)">
            <img src="~/Content/assets/images/customer/bookmark-filled.svg" class="h-[20px] w-[25px]">
            <span class="text-[#BCB08A] text-[9px] font-medium" style="line-height: 100%;">保存</span>
        </div>
    }

</div>
<div class="px-4 py-[12px]">
    <div class="flex gap-[14px] pb-2">
        <div class="flex  text-base  items-center" id="comment_ID">
            @{
                var unratedScale = (5 - Model.AverageRating);
                for (int i = 0; i < Model.AverageRating; i++)
                {
                    <svg key={ind} xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 18 18" fill="none">
                        <path d="M16.3501 6.76236L10.8678 6.18339L8.62355 1.14954L6.37926 6.18339L0.896973 6.76236L4.99185 10.453L3.84865 15.8448L8.62355 13.091L13.3984 15.8448L12.2553 10.453L16.3501 6.76236Z"
                              fill="#F89336" />
                    </svg>
                }
                for (int i = 0; i < unratedScale; i++)
                {
                    <svg xmlns="http://www.w3.org/2000/svg" width="19" height="18" viewBox="0 0 19 18" fill="none">
                        <g opacity="0.3">
                            <path d="M9.44612 1.1453L8.95768 2.25692L7.20604 6.18126L2.928 6.63601L1.71533 6.77076L2.62484 7.5792L5.8081 10.4593L4.91544 14.67L4.6628 15.849L5.72389 15.2426L9.44612 13.0868L13.1684 15.2426L14.2294 15.849L13.9768 14.67L13.0841 10.4593L16.2674 7.5792L17.1769 6.77076L15.9642 6.63601L11.6862 6.18126L9.93456 2.25692L9.44612 1.1453ZM9.44612 3.7896L10.8272 6.9055L10.962 7.19182L11.2651 7.22551L14.6505 7.5792L12.1241 9.85296L11.8883 10.0719L11.9557 10.3751L12.6631 13.6931L9.7156 12.0088L9.44612 11.8404L9.17664 12.0088L6.22917 13.6931L6.93656 10.3751L7.00393 10.0719L6.76813 9.85296L4.24173 7.5792L7.62711 7.22551L7.93028 7.19182L8.06502 6.9055L9.44612 3.7896Z"
                                  fill="#C8B477" />
                        </g>
                    </svg>
                }
            }

            <div class="text-[#333] text-[12px] font-normal " style="line-height: 18px;">@Model.AverageRating</div>
        </div>
        <div class="flex gap-1 text-base items-center">
            <svg xmlns="http://www.w3.org/2000/svg" width="17" height="16" viewBox="0 0 17 16" fill="none">
                <path d="M1.52539 2.90918V12.8488H6.35188L8.07085 14.5455L9.78981 12.8488H14.6163V2.90918H1.52539ZM2.53238 3.90314H13.6093V11.8549H9.37286L8.07085 13.14L6.76884 11.8549H2.53238V3.90314ZM4.54637 5.39409V6.38805H11.5953V5.39409H4.54637ZM4.54637 7.38202V8.37598H11.5953V7.38202H4.54637ZM4.54637 9.36994V10.3639H9.58134V9.36994H4.54637Z"
                      fill="#BCB08A" />
            </svg>
            <div class="text-[#333] text-[12px] font-normal">@Model.TotalComment</div>
        </div>
    </div>
    <div class="text-[12px] font-normal leading-[18px] self-stretch	text-[#333]">
        @Model.ClubDescription
    </div>
</div>
<!-- calender -->
<div>
    <div class=" text-[10px] flex border-t border-b border-[#EFEFEF] justify-between px-[17px] border-custom">
        @foreach (var item2 in Model.ClubWeeklyScheduleList)
        {
            if (!string.IsNullOrEmpty(item2.EnglishDay) && item2.EnglishDay.ToUpper().Trim() == "SAT")
            {
                <div class="text-blue">
                    @item2.DayLabel
                </div>
            }
            else if (!string.IsNullOrEmpty(item2.EnglishDay) && item2.EnglishDay.ToUpper().Trim() == "SUN")
            {

                <div class="text-red">
                    @item2.DayLabel
                </div>
            }
            else if (!string.IsNullOrEmpty(item2.ScheduleLabel) && item2.ScheduleLabel.ToUpper().Trim() == "DAYOFF")
            {

                <div style="background: #D9D9D9; opacity:0.3">
                    @item2.DayLabel
                </div>
            }
            else
            {
                <div>
                    @item2.DayLabel
                </div>
            }
        }
    </div>
    <div class="text-[10px] flex border-t border-b border-[#EFEFEF] justify-between px-[17px] border-custom">
        @foreach (var item2 in Model.ClubWeeklyScheduleList)
        {
            var imageURL = "/Content/assets/images/" + item2.ScheduleLabel + ".svg";
            if (!string.IsNullOrEmpty(item2.Schedule) && item2.Schedule.Trim().ToUpper() == "RESERVABLE")
            {
                @*<div onclick="ClubReservationActionFunction('@Model.ClubId', '@Model.ClubNameEng','@item2.FullDate')">*@
                <div onclick="InitiateClubReservationFunction('@Model.ClubId','@item2.FullDate' )">
                    @item2.Date <div class="flex justify-center mt-1">
                        <img src="@imageURL" class="h-[11px] w-[11px]">
                    </div>
                </div>
            }
            else
            {
                <div style="background: #D9D9D9; opacity:0.3">
                    @item2.Date <div class="flex justify-center mt-1">
                        <img src="@imageURL" class="h-[11px] w-[11px]">
                    </div>
                </div>
            }
        }
    </div>
</div>
<div class="">
    <div class="flex px-[16px] gap-[24px]" style="border-bottom: 4px solid #EFEFEF;">
        <div class="flex tab-container-plane no-scrollbar bg-white gap-[14px] overflow-x-auto" style="overflow-y:hidden">
            <button id="tab1Button" class="tab-button-plane tab-link whitespace-nowrap active">店舗トップ</button>
            <button id="tab2Button" class="tab-button-plane tab-link  whitespace-nowrap">ホスト</button>
            <button id="tab3Button" class="tab-button-plane tab-link whitespace-nowrap">レビュー</button>
            <button id="tab4Button" class="tab-button-plane tab-link whitespace-nowrap ">写真</button>
            <button id="tab5Button" class="tab-button-plane tab-link whitespace-nowrap">スケジュール</button>
            <!-- notice button -->
            <button id="tab6Button" class="tab-button-plane tab-link whitespace-nowrap">お知らせ</button>
        </div>
    </div>
</div>
<div class="relative" style="margin-bottom:65px">
    <div class="bg-white">
        <div id="tab1" class="tab-content-plane">
            <Style>
                .box {
                    position: relative;
                }

                    .box::after {
                        content: '';
                        position: absolute;
                        bottom: 0;
                        left: -16px;
                        margin: 0;
                        width: calc(100vw + 20x) !important;
                        /* width: calc(100vw + 29px) !important;*/
                        /*width:100%;*/
                        height: 6px;
                        top: 5px;
                        background-color: #EFEFEF;
                    }

                .custom-table {
                    border-collapse: collapse;
                    width: 100%;
                    border: none;
                    /* Remove outer border */
                }

                    .custom-table tr:first-child td,
                    .custom-table tr:last-child td {
                        border-bottom: none;
                        /* Remove bottom border from first and last rows */
                    }

                    .custom-table td,
                    .custom-table th {
                        border: 1px solid #EFEFEF;
                        /* Apply border to inner cells */
                        font-size: 11px;
                        /* color: #333; */
                        padding-left: 13px;
                        white-space: nowrap;
                    }

                    .custom-table th {
                        border-top: none;
                        /* Remove top border from header cells */
                        border-right: none;
                        /* Remove top border from header cells */
                    }
            </Style>
