                                        }
                                        else
                                        {
                                            <div class="text-[10px] font-bold text-primaryDark"></div>
                                        }
                                        @if (!string.IsNullOrEmpty(item.HostNameJapanese))
                                        {
                                            <h3 class="text-xs  text-charcoalBlack" style="font-weight: bold; font-size: 13px; text-overflow: ellipsis; white-space: nowrap; overflow: hidden;">@item.HostNameJapanese</h3>

                                        }
