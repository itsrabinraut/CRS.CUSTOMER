                                        {
                                            <div class="pl-6">-</div>
                                        }
                                        <div class="pl-6">@item.Rank</div>
                                    </div>
                                    <div class="relative h-[180px] w-[128px]">
                                        @if (!string.IsNullOrEmpty(item.HostImage))
                                        {
                                            <img src="@item.HostImage"
                                                 class="w-[128px] h-[180px] object-fill rounded-[6px] image-cover" onclick="window.location.href='/LocationManagement/ViewHostDetail?HostId=@item.HostId'">
                                        }
                                        else
                                        {
                                            <img src="~/Content/assets/images/customer/demo-image.jpeg"
                                                 class="w-[128px] h-[180px] object-fill rounded-[6px] image-cover" />
                                        }
                                        @if (item.IsBookmarked.Trim().ToUpper() == "Y")
                                        {
                                            <div class="absolute top-2 right-2" style="width:26px; height:26px;" onclick="ManageBookmark('@item.ClubId','@item.HostId','host', this)">
                                                <img src="~/Content/assets/images/customer/favorite-filled.svg" alt="filled" style="height:26px; width:26px" />
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="absolute top-2 right-2" style="width: 26px; height:26px;" onclick="ManageBookmark('@item.ClubId','@item.HostId','host', this)">
                                                <img src="~/Content/assets/images/customer/favorite-unfilled.svg" alt="unfilled" style="height:26px; width:26px" />
                                            </div>
                                        }

                                    </div>
                                    <div class="pt-[5px]">
                                        @if (!string.IsNullOrEmpty(item.HostPosition))
                                        {
