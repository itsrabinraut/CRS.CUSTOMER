                            <img src="@item.HostImage"
                                 class="w-[104px] h-[104px] object-cover rounded-[6px] object-cover" />
                        }
                        else
                        {
                            <img src="~/Content/assets/images/customer/demo-image.jpeg"
                                 class="w-[104px] h-[104px] object-cover rounded-[6px]" />
                        }
                        <div class="pt-[5px]">
                            <div class="text-[10px] font-bold text-primaryDark">@item.HostPosition</div>
                            <div class="text-xs  text-charcoalBlack">@item.HostName</div>
                        </div>
                    </div>
                }
            </div>
