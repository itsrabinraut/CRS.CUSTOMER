@model CRS.CUSTOMER.APPLICATION.Models.LocationManagement.ClubDetailModel
@{
    ViewBag.Title = "Club Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
    DateTime todayDate = DateTime.Now;
    string formattedDate = todayDate.ToString("MMMM yyyy");
}
<style>
    /* The Modal (background) */
    .my-custom-modal {
        display: none; /* Hidden by default */
        position: fixed; /* Stay in place */
        z-index: 1; /* Sit on top */
        padding-top: 150px; /* Location of the box */
        left: 0;
        top: 0;
        width: 100%; /* Full width */
        height: 100%; /* Full height */
        overflow: auto; /* Enable scroll if needed */
        background-color: rgb(0,0,0); /* Fallback color */
        background-color: rgba(0,0,0,0.9); /* Black w/ opacity */
    }

    /* Modal Content (image) */
    .my-modal-content {
        margin: auto;
        display: block;
        width: 80%;
        max-width: 700px;
    }

    /* The Close Button */
    .close {
        position: absolute;
        top: 50px;
        right: 35px;
        color: #f1f1f1;
        font-size: 40px;
        font-weight: bold;
        transition: 0.3s;
    }

        .close:hover,
        .close:focus {
            color: #bbb;
            text-decoration: none;
            cursor: pointer;
        }
</style>
<!-- breadcrumb  -->
<div class="flex py-[8px] px-4 gap-1 items-center breadcrumb">
    <a href="@Url.Action("Index","DashboardV2")" class="active">
        @CRS.CUSTOMER.APPLICATION.Resources.Resource.Home
    </a>
    <i class="las la-angle-right"></i>
    <a href="@Url.Action("Index", "LocationManagement", new {LocationId = Model.LocationId})" class="active">
        @CRS.CUSTOMER.APPLICATION.Resources.Resource.Clubs
    </a>
    <i class="las la-angle-right"></i>
    <a href="#">@Model.ClubNameEng</a>
</div>
<div>
    <img src="@Model.ClubCoverPhoto" alt="banner" class="h-[190px] w-full object-cover" />
</div>
<!-- section 1 -->
<div class="px-2 pt-[12px] pb-2 flex gap-0.5">
    @if (!string.IsNullOrEmpty(Model.Tag1))
    {
        <div class="px-[10px] bg-[#E3A1A1] text-[10px] rounded-[2px] text-white">@Model.Tag1</div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag2))
    {
        <div class="px-[10px] bg-[#BFA3B6] text-[10px] rounded-[2px] text-white">@Model.Tag2</div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag3))
    {
        <div class="px-[10px] bg-[#697DBF] text-[10px] rounded-[2px] text-white">@Model.Tag3</div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag4))
    {
        <div class="px-[10px] bg-[#96B41F] text-[10px] rounded-[2px] text-white">@Model.Tag4</div>
    }
    @if (!string.IsNullOrEmpty(Model.Tag5))
    {
        <div class="px-[10px] bg-[#EFB726] text-[10px] rounded-[2px] text-white">@Model.Tag5</div>
    }
</div>
<!-- section 2 -->
<div class="px-4 pb-[12px] flex justify-between border-b-2 border-[#FAFAFA]">
    <div class="flex gap-[8px]">
        <div>
            <img src="@Model.ClubLogo"
                 class="h-[48px] w-[48px] object-cover rounded-full object-cover" />
        </div>
        <div>
            <div class='text-sm font-bold text-charcolBlack '>@Model.ClubNameEng</div>
            <div class='text-xs  text-[#404040] '>@Model.ClubNameJp</div>
            <div class="text-[10px] text-primary-softrose">@Model.GroupName</div>
        </div>
    </div>
    @if (Model.IsBookmarked.ToUpper() == "N")
    {
        <div class="flex items-center cursor-pointer" onclick="handleAddBookmarks('@Model.ClubId', '', 'club')">
            <img src="~/Content/assets/images/customer/bookmark-outlined.svg" class="h-[20px] w-[25px]" />
        </div>
    }
    else
    {
        <div class="flex items-center cursor-pointer" onclick="handleRemoveBookmarks('@Model.ClubId', '', 'club')">
            <img src="~/Content/assets/images/customer/bookmark-filled.svg" class="h-[20px] w-[25px]">
        </div>
    }

</div>
<div class="px-4 py-[12px]">
    <div class="flex gap-[14px] pb-2">
        <div class="flex gap-1 text-base  items-center">
            @{
                var unratedScale = (5 - Model.AverageRating);
                for (int i = 0; i < Model.AverageRating; i++)
                {
                    <svg key={ind} xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 18 18" fill="none">
                        <path d="M16.3501 6.76236L10.8678 6.18339L8.62355 1.14954L6.37926 6.18339L0.896973 6.76236L4.99185 10.453L3.84865 15.8448L8.62355 13.091L13.3984 15.8448L12.2553 10.453L16.3501 6.76236Z"
                              fill="#F89336" />
                    </svg>
                }
                for (int i = 0; i < unratedScale; i++)
                {
                    <svg xmlns="http://www.w3.org/2000/svg" width="19" height="18" viewBox="0 0 19 18" fill="none">
                        <g opacity="0.3">
                            <path d="M9.44612 1.1453L8.95768 2.25692L7.20604 6.18126L2.928 6.63601L1.71533 6.77076L2.62484 7.5792L5.8081 10.4593L4.91544 14.67L4.6628 15.849L5.72389 15.2426L9.44612 13.0868L13.1684 15.2426L14.2294 15.849L13.9768 14.67L13.0841 10.4593L16.2674 7.5792L17.1769 6.77076L15.9642 6.63601L11.6862 6.18126L9.93456 2.25692L9.44612 1.1453ZM9.44612 3.7896L10.8272 6.9055L10.962 7.19182L11.2651 7.22551L14.6505 7.5792L12.1241 9.85296L11.8883 10.0719L11.9557 10.3751L12.6631 13.6931L9.7156 12.0088L9.44612 11.8404L9.17664 12.0088L6.22917 13.6931L6.93656 10.3751L7.00393 10.0719L6.76813 9.85296L4.24173 7.5792L7.62711 7.22551L7.93028 7.19182L8.06502 6.9055L9.44612 3.7896Z"
                                  fill="#C8B477" />
                        </g>
                    </svg>
                }
            }
            <div class="text-secondary-1">@Model.AverageRating</div>
        </div>
        <div class="flex gap-1 text-base items-center">
            <i class="las la-comment text-[#BCB08A]"></i>
            <div class="text-secondary-1">@Model.TotalComment</div>
        </div>
    </div>
    <div class="text-sm leading-4	text-secondary-1">
        @Model.ClubDescription
    </div>
</div>
<div>
    <div class=" text-[10px] flex border-t border-b border-[#EFEFEF] justify-between px-[17px] border-custom">
        @foreach (var item2 in Model.ClubWeeklyScheduleList)
        {
            if (!string.IsNullOrEmpty(item2.EnglishDay) && item2.EnglishDay.ToUpper().Trim() == "SAT")
            {
                <div class="text-blue">
                    @item2.DayLabel
                </div>
            }
            else if (!string.IsNullOrEmpty(item2.EnglishDay) && item2.EnglishDay.ToUpper().Trim() == "SUN")
            {

                <div class="text-red">
                    @item2.DayLabel
                </div>
            }
            else
            {
                <div>
                    @item2.DayLabel
                </div>
            }
        }
    </div>
    <div class="text-[10px] flex border-t border-b border-[#EFEFEF] justify-between px-[17px] border-custom">
        @foreach (var item2 in Model.ClubWeeklyScheduleList)
        {
            var imageURL = "/Content/assets/images/" + item2.Schedule + ".svg";
            if (!string.IsNullOrEmpty(item2.Schedule) && item2.Schedule.Trim().ToUpper() == "RESERVABLE")
            {
                @*<div onclick="ClubReservationActionFunction('@Model.ClubId', '@Model.ClubNameEng','@item2.FullDate')">*@
                <div onclick="InitiateClubReservationFunction('@Model.ClubId','@item2.FullDate' )">
                    @item2.Date <div class="flex justify-center mt-1">
                        <img src="@imageURL" class="h-[11px] w-[11px]">
                    </div>
                </div>
            }
            else
            {
                <div>
                    @item2.Date <div class="flex justify-center mt-1">
                        <img src="@imageURL" class="h-[11px] w-[11px]">
                    </div>
                </div>
            }
        }
    </div>
</div>
<div class="px-4 py-[12px] flex justify-between items-center">
    <div>
        @if (!string.IsNullOrEmpty(Model.ClubOpeningTime) && !string.IsNullOrEmpty(Model.ClubClosingTime))
        {
            <div class="flex justify-center items-center gap-1 pb-1">
                @if (!string.IsNullOrEmpty(Model.ClubTimePeriod) && Model.ClubTimePeriod.Trim().ToUpper() == "DAY")
                {
                    <div class="h-4 w-4 rounded-[2px] bg-[#F89336] flex justify-center items-center">
                        <img src="~/Content/assets/images/customer/sun.svg" />
                    </div>
                }
                else if (!string.IsNullOrEmpty(Model.ClubTimePeriod) && Model.ClubTimePeriod.Trim().ToUpper() == "NIGHT")
                {
                    <div class="h-4 w-4 rounded-[2px] bg-[#F89336] flex justify-center items-center">
                        <img src="~/Content/assets/images/customer/moon.svg" />
                    </div>
                }
                <div class="text-sm text-secondary-1">@Model.ClubOpeningTime~@Model.ClubClosingTime</div>
            </div>
            if (!string.IsNullOrEmpty(Model.TodaysClubSchedule) && Model.TodaysClubSchedule.Trim().ToUpper() == "RESERVABLE")
            {
                <div class="bg-[#BCB08A] text-white w-max px-2 rounded-[2px] text-xs h-[18px] flex items-center justify-center">
                    @CRS.CUSTOMER.APPLICATION.Resources.Resource.Reservable
                </div>
            }
            else
            {
    <div class="bg-[#D7D7D7] text-white w-max px-2 rounded-[2px] text-xs h-[18px] flex items-center justify-center">
        @CRS.CUSTOMER.APPLICATION.Resources.Resource.Reservable
    </div>
            }
        }
    </div>
    @*<button class="btn-gradient" onclick="ClubReservationActionFunction('@Model.ClubId', '@Model.ClubNameEng')">*@
    <button class="btn-gradient" onclick="InitiateClubReservationFunction('@Model.ClubId')">
        @CRS.CUSTOMER.APPLICATION.Resources.Resource.BookNow
    </button>
</div>
<div class="divider-customer"></div>
<div class="">
    @*<div class="flex px-[16px] gap-[24px] w-full overflow-hidden overflow-x-scroll no-scrollbar" style="border-bottom: 4px solid #EFEFEF;">*@
