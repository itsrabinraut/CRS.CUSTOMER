@section scripts
{
    <script>
        (function () {
            const container = document.getElementById('host-search-result-scroll');

            let isDragging = false;
            let startX, scrollLeft;
            const links = container.querySelectorAll('a');

            container.addEventListener('mousedown', (e) => {
                isDragging = true;
                startX = e.pageX - container.offsetLeft;
                scrollLeft = container.scrollLeft;
                container.classList.add('disable-pointer-events'); // Disable pointer events during drag
            });

            container.addEventListener('mouseleave', () => {
                if (isDragging) {
                    isDragging = false;
                    container.classList.remove('disable-pointer-events'); // Re-enable pointer events
                }
            });

            container.addEventListener('mouseup', () => {
                if (isDragging) {
                    isDragging = false;
                    container.classList.remove('disable-pointer-events'); // Re-enable pointer events
                }
            });

            container.addEventListener('mousemove', (e) => {
                if (!isDragging) return;
                e.preventDefault();
                const x = e.pageX - container.offsetLeft;
                const walk = (x - startX) * 2; // Scroll-fastness factor
                container.scrollLeft = scrollLeft - walk;
            });

            // Optionally, if you want to handle clicks while dragging is ongoing
            links.forEach(link => {
                link.addEventListener('click', (e) => {
                    if (container.classList.contains('disable-pointer-events')) {
                        e.preventDefault(); // Prevent link click if dragging
                    }
                });
            });
        })();
    </script>
    <script>
        let currentPage = Math.floor((@ViewBag.StartIndex) / 12) + 1; // Calculate current page number
        const totalPages = @(ViewBag.TotalRecords != null ? Math.Ceiling(Convert.ToDouble(ViewBag.TotalRecords) / 12) : 0);
        var prefecturesArea = "@ViewBag.PrefecturesArea";
        var typeValue = "";
        if ("@ViewBag.TypeValue" == "03") {
            typeValue = "scftab=03&NewHost=true";
        } else {
            typeValue = "scftab=02";
        }
        var page_size = 12;
        initializePagination(currentPage, totalPages, prefecturesArea, typeValue, page_size);
        scrollToTop();
        //adjustFilterHeight();
    </script>
}
