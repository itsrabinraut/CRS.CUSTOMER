@section scripts
{
    <script>
        $(document).ready(async function () {
            var Value1 = @Html.Raw(Json.Encode(ViewBag.Date));
            var Value2 = @Html.Raw(Json.Encode(ViewBag.Time));
            var Value3 = @Html.Raw(Json.Encode(ViewBag.NoOfPeople));
            if ((Value1 != null && Value1 != "") ||
                (Value2 != null && Value2 != "") ||
                (Value3 != null && Value3 != "")) {
                const result = await InitiateDateTimeFilterPopupFunction();
                document.body.classList.remove('body-no-scroll');
                initTimeFunction2();
                initPeopleFunction2();
                var element = document.getElementById('drawer-date-time');
                if (element) {
                    element.classList.add('translate-y-full');
                }
            }
        });
    </script>
    <script>
        (function () {
            const container = document.getElementById('recomended-club');
            let isDragging = false;
            let startX, scrollLeft;

            container.addEventListener('mousedown', (e) => {
                isDragging = true;
                startX = e.pageX - container.offsetLeft;
                scrollLeft = container.scrollLeft;
            });

            container.addEventListener('mouseleave', () => {
                isDragging = false;
            });

            container.addEventListener('mouseup', () => {
                isDragging = false;
            });

            container.addEventListener('mousemove', (e) => {
                if (!isDragging) return;
                e.preventDefault();
                const x = e.pageX - container.offsetLeft;
                const walk = (x - startX) * 2; // Scroll-fastness factor
                container.scrollLeft = scrollLeft - walk;
            });
        })();
    </script>
    <script>
        let currentPage =  Math.floor((@ViewBag.StartIndex) / 25) + 1;
        const totalPages = @(ViewBag.TotalRecords != null ? Math.Ceiling(Convert.ToDouble(ViewBag.TotalRecords) / 25) : 0);
        var prefecturesArea = "@ViewBag.PrefecturesArea";
        var typeValue = "";
        if ("@ViewBag.TypeValue" == "true") {
            var typeValue = "";
        } else {
            typeValue = "scftab=01";
        }
       
        var page_size = 25;
        initializePagination(currentPage, totalPages, prefecturesArea, typeValue, page_size);
        scrollToTop();
        adjustFilterHeight();
    </script>
}