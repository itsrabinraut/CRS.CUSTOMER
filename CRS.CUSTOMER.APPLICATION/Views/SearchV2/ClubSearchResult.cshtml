<!-- divider -->
@if(Model.RecommendedClubModel.Count > 0)
            {

<div class="bg-[#F1F1F1] mt-[8px]">
    <div class=" border-b border-[#EDEDED]" style="background:#E3E3E3;">
        <div class="flex mt-[8px]   justify-start mx-[16px] flex-row" style="padding-top:12px">
            <div class="  flex flex-row justify-start items-center  ">
                <div class="flex flex-col">
                    <div class="font-[700] text-[#333] text-[14px] leading-[20.27px]">オススメのホストクラブ</div>
                </div>
            </div>
        </div>
        <div class="flex  overflow-x-auto flex-row gap-[8px] mx-[16px] my-[12px] no-scrollbar mb-[12px]" id="recomended-club">
            @{
                foreach (var item in Model.RecommendedClubModel)
                {
                    <a onclick="ClubDetailRedirectFunction('@item.ClubCode', '@item.LocationURL')">
                        <div class="flex  w-[118px]">
                            <div style="border-radius: 6px; border: 1px solid #F3F3F3; background: #FFF;"
