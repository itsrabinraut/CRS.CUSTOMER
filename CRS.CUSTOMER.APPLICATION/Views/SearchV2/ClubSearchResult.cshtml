            @{
                var disableClickClass = "disable-click";
                disableClickClass = (!string.IsNullOrEmpty(ViewBag.Date) || !string.IsNullOrEmpty(ViewBag.Time) || !string.IsNullOrEmpty(ViewBag.NoOfPeople)) ? "" : disableClickClass;
                <div style="margin-left:15px;margin-right:15px; margin-bottom: 24px;" class="flex flex-row justify-center rounded-[8px] bg-[#D75A8B] mt-[16px] @disableClickClass" onclick="SubmitDateTimeFilterFunction();" id="DateTimeFilter-Id">
