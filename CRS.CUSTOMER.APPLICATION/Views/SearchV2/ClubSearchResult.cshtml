<!-- divider -->
<div class="bg-[#F1F1F1] mt-[8px]">
    <div class="border-t border-b border-[#EDEDED]" style="background:#E3E3E3;">
        <div class="flex mt-[12px]  justify-start mx-[16px] flex-row">
            <div class="  flex flex-row justify-start items-center  ">
                <div class="flex flex-col">
                    <div class="font-[700] text-[#333] text-[14px] leading-[20.27px]">あなたにオススメのホストクラブ</div>
                </div>
            </div>
        </div>
        <div class="flex  overflow-x-auto flex-row gap-[8px] mx-[16px] my-[12px] no-scrollbar mb-[12px]">
            @{
                foreach (var item in Model.RecommendedClubModel)
                {
                    <a onclick="ClubDetailRedirectFunction('@item.ClubCode', '@item.LocationURL')">
                        <div class="flex  w-[118px]">
                            <div style="border-radius: 6px; border: 1px solid #F3F3F3; background: #FFF;"
                                 class=" flex flex-col w-[118px]  px-[8px] py-[10px]">
                                <div class="flex flex-row justify-center">

                                    <div class="h-[48px] w-[48px] object-fill   bg-[#ffffff]  flex flex-row justify-center">
                                        <img style="border-color:#F1EEEE" class="h-[48px] w-[48px] object-fill border-[1px]   rounded-full "
                                             src="@item.ClubLogo" alt="clublogo">
                                    </div>
                                </div>
                                <div class="flex flex-col">
                                    <div class="flex flex-row mt-[7px] justify-center">
                                        <p style="line-height: 100%;" class="font-[700] text-[10px] text-clip">@item.ClubNameEnglish</p>
                                    </div>
                                    <div class="flex flex-row mt-[4px] justify-center">
                                        <p style="line-height: 100%;" class="font-[400] text-[10px] text-clip">@item.ClubNameJapanese</p>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </a>
                }
            }
        </div>
    </div>
</div>

<!-- for search result -->
<div class="bg-[#F1EEEE] flex justify-start gap-[5px] py-2 px-4 text-[#292F33] font-semibold text-[12px]">
    <span>検索結果</span>
    <span>@Model.FilteredClubModel.Count 件</span>
</div>
<div class="bg-white">
    <!-- club section  -->
    <div id="tab1" class="tab-content-plane  ">
        @{
            if (Model.FilteredClubModel.Count <= 0)
            {
                <div class="flex flex-col gap-4 p-4  border-t border-t-[#E0E0E0] flex justify-center items-center">
                    <div class="text-[#18181B] font-semibold mt-12 text-center">
                        @CRS.CUSTOMER.APPLICATION.Resources.Resource.NoSearchRecordFound
                    </div>
                </div>
            }
            else
            {
                foreach (var item in Model.FilteredClubModel)
                {
                    /*new design implemented*/

                    <div class="club_content">
                        <!-- s/link -->
                        <a onclick="ClubDetailRedirectFunction('@item.ClubCode', '@item.LocationURL')">
                            <!-- s/club_tag -->
                            <ul class="club_tags">
                                @if (!string.IsNullOrEmpty(@item.Tag1))
                                {
                                    <!-- s/tag -->
                                    <li class="tag_location"><img class="map_pin" src="~/Content/assets/images/location_icon.svg" alt="ピン"> @item.Tag1</li>
                                    <!-- e/tag -->
                                }
                                @if (!string.IsNullOrEmpty(@item.Tag2))
                                {
                                    <!-- s/tag -->
                                    <li class="tag_rank"> @item.Tag2</li>
                                }
                                <!-- e/tag -->
                                <!-- s/tag -->
                                @if (!string.IsNullOrEmpty(@item.Tag3))
                                {
                                    <li class="tag_platinum">  @item.Tag3</li>
                                }

                                @if (!string.IsNullOrEmpty(@item.Tag4))
                                {
                                    <!-- e/tag -->
                                    <!-- s/tag -->
                                    <li class="tag_good">@item.Tag4</li>
                                }
                                @if (!string.IsNullOrEmpty(@item.Tag5))
                                {
                                    <li class="tag_big">  @item.Tag5</li>
                                }
                                <!-- e/tag -->
                            </ul>
                            <!-- e/club_tag -->
                            <!-- s/club header -->
                            <div class="club_header">
                                <!-- s/club logo -->
                                <div class="club_logo"><img class="logo" alt="ロゴ" src="@item.ClubLogo"></div>
                                <!-- e/club logo -->
                                <!-- s/club info -->
                                <div class="club_info">
                                    <!-- s/club_name -->
                                    <h3 class="club_name">@item.ClubNameEnglish</h3>
                                    <!-- e/club_name -->
                                    <!-- s/club kana -->
                                    <p class="club_kana">@item.ClubNameJapanese</p>
                                    <!-- e/club kana -->
                                    <!-- s/club group -->
                                    <p class="club_group">@item.GroupName</p>
                                    <!-- e/club group -->
                                </div>
                                <!-- e/club info -->
                                <!-- s/action btn -->
                                @if (item.IsBookmarked.ToUpper() == "Y")
                                {
                                    <div class="action_keep" onclick="event.stopPropagation(); ManageBookmark('@item.ClubId', '', 'club', this, '@PostURL')"><img alt="保存する" src="~/Content/assets/images/bookmark-filled.svg"></div>
                                }
                                else
                                {
                                    <div class="action_keep" onclick="event.stopPropagation(); ManageBookmark('@item.ClubId','','club', this, '@PostURL')"><img alt="保存する" src="~/Content/assets/images/bookmark-notfilled.svg"></div>
                                }
                                <!-- e/action btn -->
                            </div>
                            <!-- e/club header -->
                            <!-- s/thumb area -->
                            <ul class="club_thumb">
                                <!-- s/club thumb -->
                                @foreach (var item2 in item.HostGalleryImage)
                                {
                                    <li><img alt="@item.ClubNameEnglish ([ruby])の写真‖[area]([prefectures])のホストクラブ‖ホスログ《ホストクラブ初回来店・ネット予約》" src="@item2" style="object-position:top;"></li>
                                }
                                <!-- e/club thumb -->
                                <!-- s/club thumb -->
                                <!-- e/club thumb -->
                            </ul>
                            <!-- e/thumb area -->
                            <!-- s/club rate -->
                            <div class="club_rate">
                                <!-- s/club rank -->
                                <ul class="rate_stars">
                                    <!-- s/rank star -->
                                    @{
                                        var unratedScale = (5 - item.AverageRating);
                                        for (int i = 0; i < item.AverageRating; i++)
                                        {
                                            <li><img src="~/Content/assets/images/ratingstar-filled.svg" alt="未評価"></li>
                                        }
                                        for (int i = 0; i < unratedScale; i++)
                                        {
                                            <!-- e/rank star -->
                                            <!-- s/rank star -->
                                            <li><img src="~/Content/assets/images/ratingstar-notfilled.svg" alt="未評価"></li>
                                        }
                                    }
                                    <!-- e/rank star -->
                                    <!-- s/rank star -->
                                    <!-- e/rank star -->
                                    <!-- s/rank star -->
                                    <li class="rate_value"> @item.AverageRating</li>
                                    <!-- e/rank star -->
                                </ul>
                                <!-- e/club rank -->
                                <!-- s/club review -->
                                <ul class="rate_review">
                                    <!-- s/comment frame -->
                                    <li><img src="~/Content/assets/images/comment.svg" alt="コメント"></li>
                                    <!-- e/comment frame -->
                                    <!-- s/comment value -->
                                    <li class="comment_value">@item.TotalComment</li>
                                    <!-- e/comment value -->
                                </ul>
                                <!-- e/club review -->
                            </div>
                            <!-- e/club rate -->
                            <!-- s/club pr -->
                            <p class="club_pr"> @item.ClubDescription</p>
                            <!-- e/club pr -->
                        </a>
                        <!-- e/link -->
                        <!-- s/club footer -->
                        <div class="club_footer">
                            <!-- s/club status -->
                            <div class="club_status">
                                <!-- s/opening hours -->
                                @if (!string.IsNullOrEmpty(@item.ClubOpeningTime) && !string.IsNullOrEmpty(@item.ClubClosingTime))
                                {
                                    if (!string.IsNullOrEmpty(@item.ClubTimePeriod) && @item.ClubTimePeriod.Trim().ToUpper() == "DAY")
                                    {
                                        <div class="opening_hours"><img src="~/Content/assets/images/customer/sun.svg" alt="夜"> @item.ClubOpeningTime &#xff5e; @item.ClubClosingTime</div>
                                    }
                                    else if (!string.IsNullOrEmpty(@item.ClubTimePeriod) && @item.ClubTimePeriod.Trim().ToUpper() == "NIGHT")
                                    {
                                        <div class="opening_hours"><img src="/Content/assets/images/customer/moon.svg" alt="夜">@item.ClubOpeningTime &#xff5e; @item.ClubClosingTime</div>
                                    }

                                    <!-- e/opening hours -->
                                    <!-- s/available -->
                                    if (!string.IsNullOrEmpty(item.TodaysClubSchedule) && item.TodaysClubSchedule.Trim().ToUpper() == "RESERVABLE")
                                    {
                                        <div class="available">本日空席あり</div>
                                    }
                                    else
                                    {
                                        <div class="available disabled">本日空席あり</div>
                                    }
                                }
                                <!-- e/available -->
                            </div>
                            <!-- e/club status -->
                            <!-- s/reservation btn -->
                            <button class="rsv_btn gradient" onclick="InitiateClubReservationFunction('@item.ClubId', '','','@PostURL')">
                                @CRS.CUSTOMER.APPLICATION.Resources.Resource.Book_Now
                                @* 今すぐ予約する*@
                            </button>
                            <!-- e/reservation btn -->
                        </div>
                        <!-- e/club footer -->
                    </div>
                    /*New design end*/
                    <div style="background-color: #e8e8e8;" class=" h-[8px] my-[0px] py-[0px]">
                    </div>
                }
            }
        }
        <div class=" ">
            <div>
            </div>
            <div style="background-color: #FFFFFF;"
                 class="flex flex-row items-center justify-end gap-1 pr-[16px] my-[12px]">
                <div class="flex flex-row items-center justify-end gap-1 pr-[16px] " id="scrollToTopButton">
