@section scripts
{
    <script>
        let currentPage = Math.floor((@ViewBag.StartIndex) / 12) + 1; // Calculate current page number
        const totalPages = @(ViewBag.TotalRecords != null ? Math.Ceiling(Convert.ToDouble(ViewBag.TotalRecords) / 12) : 0);
        var prefecturesArea = "@ViewBag.PrefecturesArea";
        var typeValue = "host";
        var page_size = 12;
        initializePagination(currentPage, totalPages, prefecturesArea, typeValue, page_size);
        scrollToTop();
        adjustFilterHeight();
        function redirectToSearchPage() {
            var prefecturesArea = '@ViewBag.PrefecturesArea';
            var searchUrl = `/search${prefecturesArea}/?target=club`;
            window.location.href = searchUrl;
        }
    </script>

    <script>
        (function () {
            const container = document.getElementById('scroll-container-host');
            let isDragging = false;
            let startX, scrollLeft;

            container.addEventListener('mousedown', (e) => {
                isDragging = true;
                startX = e.pageX - container.offsetLeft;
                scrollLeft = container.scrollLeft;
            });

            container.addEventListener('mouseleave', () => {
                isDragging = false;
            });

            container.addEventListener('mouseup', () => {
                isDragging = false;
            });

            container.addEventListener('mousemove', (e) => {
                if (!isDragging) return;
                e.preventDefault();
                const x = e.pageX - container.offsetLeft;
                const walk = (x - startX) * 2; // Scroll-fastness factor
                container.scrollLeft = scrollLeft - walk;
            });
        })();
    </script>
}