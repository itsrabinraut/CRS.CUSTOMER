                                        if (!string.IsNullOrEmpty(club.TodaysClubSchedule) && club.TodaysClubSchedule.Trim().ToUpper() == "RESERVABLE" && DateTime.Parse(club.ClubOpeningTime) < currentTime && DateTime.Parse(club.ClubClosingTime) > currentTime)
