@model CRS.CUSTOMER.APPLICATION.Models.DashboardV2.PreferenceFilterModel
<style>
    .preferance > input[type="checkbox"] {
        display: none;
    }

    /* Style the labels to look like bordered options */
    label {
        display: inline-block;
        padding: 5px 10px;
      /*  margin: 5px;*/
        border: 1px solid #d75a8b;
        border-radius: 5px;
        cursor: pointer;
    }

    /* Style the labels when they are selected */
    .preferance > input[type="checkbox"]:checked + label {
        background-color: #fff;
        border-color: #d75a8b;
        color: #d75a8b;
    }

    .preferance > input[type="checkbox"]:checked + label {
        background-color: #fff;
        border-color: #d75a8b;
        color: #d75a8b;
    }

    .preferance > input[type="checkbox"] + label {
        background-color: #fff;
        /*  border-color: #d75a8b;
        color: */ /*#d75a8b;*/
    }

    img {
        object-fit: cover;
    }
</style>
<style>
    .map-container #map {
        height: 365px;
    }

    .gm-style div[aria-label="Google Map"] > div:last-child {
        display: none;
    }

    select {
        -webkit-appearance: none; /* Safari and Chrome */
        -moz-appearance: none; /* Firefox */
        appearance: none; /* Standard syntax */
    }

        select::-ms-expand {
            display: none; /* IE10+ and Edge */
        }

    .input-search::placeholder {
        font-size: 12px;
        line-height: 40px;
        font-weight: 400;
        transform: translateY(-2px)
    }
</style>
<style>
    .preferance-tab-wrapper {
        width: 100%;
        position: relative;
    }

        .preferance-tab-wrapper::before {
            content: '';
            display: block;
            position: absolute;
            bottom: 0px;
            left: 0;
            right: 0;
            height: 3px;
            background-color: #F4F4F4;
            z-index: 0;
            transition: background-color 0.3s;
        }

        .preferance-tab-wrapper .tab-link {
            position: relative;
            padding: 10px;
            cursor: pointer;
            z-index: 1;
        }

            .preferance-tab-wrapper .tab-link::before {
                content: '';
                display: block;
                position: absolute;
                bottom: 0px;
                left: 0;
                right: 0;
                height: 3px;
                background-color: #F4F4F4;
                z-index: 0;
                transition: background-color 0.3s;
            }

            .preferance-tab-wrapper .tab-link.active::before {
                background-color: #D75A8B;
                border-radius: 10px;
            }

            .preferance-tab-wrapper .tab-link.active{
                color:#d75a8b
            }
</style>
