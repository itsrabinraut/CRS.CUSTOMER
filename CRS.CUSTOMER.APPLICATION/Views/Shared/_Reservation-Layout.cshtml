    <script>
        tailwind.config = {
            theme: {
                extend: {
                    colors: {
                        "warmgray": "#BCB08A",
                        "warmgray-1": "#a09e91",
                        "warmgray-2": "#404040",
                        "charcoalBlack": "#292F33",
                        "primary": "#D75A8B",
                        "primaryDark": "#d17979",
                        "footer": "#2C2C2C",
                        "primary-softrose": "#e3a1a1",
                        "secondary-1": "#333",
                        "blue": "#697DBF",
                        "red": "#B12D23",
