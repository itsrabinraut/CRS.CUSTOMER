        font-size: 8px !important;
    }

    .tag_platinum {
        font-size: 8px !important;
    }

    .tag_good {
        font-size: 8px !important;
    }

    .tag_big {
        font-size: 8px !important;
    }

    .loader2 {
    }
</style>


