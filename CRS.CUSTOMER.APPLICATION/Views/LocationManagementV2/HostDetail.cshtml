            <img class="object-cover h-[48px] w-[48px] rounded-full "
                 src="@Model.ClubLogo" alt="clublogo">
        </div>
        <!-- info -->
        <div class="flex flex-col gap-1 ">
            <div class="clubName">@Model.ClubNameEnglish</div>
            <div class="store mt-1">@Model.ClubNameJapanese</div>
            <div class="flex text-[#A09E91] font-medium text-[10px] gap-1 mt-[7px]">
