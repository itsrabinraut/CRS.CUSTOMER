    <script>
        const shareButton = document.querySelector("#shareButton");
        var shareableLink = window.location.href;
        shareButton.addEventListener("click", (event) => {
            if (navigator.share) {
                navigator.share({
                    title: "Share host details",
                    url: shareableLink,
                }).then(() => {
                    //console.log('Thanks for sharing!');
                }).catch(console.error);
            } else {
                toastr.info('Web Share API is not supported in this browser.');
            }
        });
        $(document).ready(function () {
            localStorage.clear();
        });
        document.addEventListener("DOMContentLoaded", function () {
            // The DOM is fully loaded, hide the loader
            document.getElementById("loader-id").style.display = "none";
        });

        // If you also want to handle external resources like images, use the load event
        window.addEventListener("load", function () {
            // The entire page, including external resources, is fully loaded, hide the loader
            document.getElementById("loader-id").style.display = "none";
        });

        function ActivateLoaderFunction() {
            var myDiv = document.getElementById("loader-id");
            myDiv.style.display = "flex";
        }
        function DeactivateLoaderFunction() {
            var myDiv = document.getElementById("loader-id");
            myDiv.style.display = "none";
        }

        function ClubReservationActionFunction(i, j, k) {
            DataStorageFunction(i, j);
            window.location.href = "/LocationManagement/ClubReservation?ClubId=" + i + "&SelectedHost=" + k;
        }

        const DataStorageFunction = (i, j) => {
            localStorage.setItem("ClubId", EncryptParameter(i));
            localStorage.setItem("ClubName", EncryptParameter(j));
        }</script>
    <script>
        // Function to open a specific modal
        function openModal(modalId) {
            var modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = "flex";
            }
        }

        // Function to close a specific modal
        function closeModal(modalId) {
            var modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = "none";
            }
        }

        // Close the modal if the overlay is clicked
        window.onclick = function (event) {
            var modals = document.getElementsByClassName("modal");
            for (var i = 0; i < modals.length; i++) {
                if (event.target == modals[i]) {
                    closeModal(modals[i].id);
                }
            }
        }

        // Open the first modal automatically when the page loads
        // window.onload = function() {
        //     openModal('modal1');
        // }
        // Close the modal only if the overlay is clicked outside of modal1
        window.onclick = function (event) {
            var modal1 = document.getElementById('modal1');
            // Check if the click is outside of modal1
            if (modal1 && event.target !== modal1 && !modal1.contains(event.target)) {
                closeModal('modal1');
            }
        }</script>


    <script>
        var skillData = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(HostSkillsModel));
        var arrayLabel = skillData.map(x => x.Label);
        var arrayValue = skillData.map(x => x.LabelValue);

        var data = {
            //labels: ["Photoshop", "イラストレーター", "microsoft office", ["Vray", "フォー ライノ"], "test", "Rhinoceros"],
            labels: arrayLabel,
            datasets: [{
                label: "",
                //data: [50, 50, 50, 100, 50, 100],
                data: arrayValue,
                borderColor: '#D75A8B',
                backgroundColor: 'rgba(215, 90, 139, 0.3)',
                borderWidth: 4,

            }]
        };

        var options = {
            plugins: {
                datalabels: {
                    display: false
                }
            },
            layout: {
                padding: {
                    left: 1,
                    right: 1,
                    // top: 10,
                    // bottom: 1
                }
            },
            scale: {
                gridLines: {
                    color: ["#FFD5E6", "#FFD5E6", "#FFBDD8", "#FF9AC3", "#FF8BBA"],
                    lineWidth: 2
                },
                ticks: {
                    display: false,
                    beginAtZero: true,
                    min: 0,
                    max: 100,
                    stepSize: 20,
                },

                angleLines: {
                    display: false
                },
            },
            legend: {
                display: false
            },

        };

        var ctx = document.getElementById("hostSkillChart").getContext("2d");

        var hostRadarChart = new Chart(ctx, {
            type: 'radar',
            data: data,
            options: options
        });

        var labels = document.querySelectorAll('#hostSkillChart > .chartjs-radar >.chartjs-legend > li > span ');
        labels.forEach(function (label) {
            label.style.overflow = 'hidden';
            label.style.textOverflow = 'ellipsis';
            label.style.whiteSpace = 'wrap';
            label.style.maxWidth = '10px'; // Set the maximum width before truncation
        });
        // Modify dataset properties
        hostRadarChart.data.datasets.forEach(function (dataset) {
            dataset.pointBackgroundColor = '#FFCFE2'; // Change the color of the spotted points
            dataset.pointBorderColor = '#FF8DBA';
            dataset.pointBorderWidth = 1;
        });

        // Update the chart
        hostRadarChart.update();
    </script>

