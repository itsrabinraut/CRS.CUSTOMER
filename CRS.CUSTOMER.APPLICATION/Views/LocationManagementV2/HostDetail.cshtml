@model CRS.CUSTOMER.APPLICATION.Models.LocationManagement.ViewHostDetailModelV2
@{
    var ClubId = ViewBag.ClubName;
    var ClubCode = ViewBag.ClubId;
    var HostName = ViewBag.HostName;
    var LocationURL = ViewBag.LocationId;
    ViewBag.Title = $"{HostName}のプロフィール‖{ClubId}‖ホスログ ||◤推しホストが見つかる◢|| ホストクラブ初回来店・ネット予約";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var HostSkillsModel = @Model.HostIdentityDetailsModel.Where(x => x.LabelType.Trim().ToUpper() == "SKILLS").ToList();
    var metaContent = $"{HostName}のプロフィールページです。業界初！ホストクラブの初回来店がネット予約出来る「ホスログ」にて、{HostName}の基本情報・メッセージ・一問一答などをチェック♪";
}
<meta name="description" content="@metaContent">

<style>
    .clubInfo {
        border-radius: 6px;
        border: 1px solid #EAEAEA;
        background: #F6F6F6;
        margin-left: 12.5px;
        margin-right: 12.5px;
        padding: 13px 0px 14px 14.5px;
    }

    .clubName {
        color: #292F33;
        font-size: 14px;
        font-style: normal;
        font-weight: 700;
        line-height: 100%;
    }

    .store {
        color: #404040;
        font-size: 12px;
        font-style: normal;
        font-weight: 400;
        line-height: 100%;
    }

    .owl-stage .owl-item.active {
        width: 295px !important;
        margin-left: 16px;
    }

    .owl-item active {
    }

    .owl-theme .owl-dots {
        margin: 0;
        display: flex;
        justify-content: center;
        gap: 5px;
    }

    .owl-carousel.owl-loaded {
        margin-top: 10px;
        /*       margin-left:5px;*/
    }

    .owl-theme .owl-nav.disabled + .owl-dots {
        margin-top: 5px !important;
        width: calc(100vw - 50px);
        margin-left: 21px;
    }

    .owl-theme .owl-dots .owl-dot {
        gap: 5px;
    }

        .owl-theme .owl-dots .owl-dot span {
            width: 100%;
            /*  max-width: 50px;*/
            margin: 0;
            background: #E0E0E0;
            height: 2px;
            border: 1px solid #CDCDCD
        }

        .owl-theme .owl-dots .owl-dot.active span {
            width: 100%;
            /*  max-width: 50px;*/
            background: #E3A1A1;
            height: 2px;
            border: 1px solid #E39696
        }

    .owl-carousel .owl-item img {
        display: block;
        width: 100%;
    }

    .owl-carousel {
        position: relative;
    }

    .owl-theme .owl-dots {
        position: absolute;
        top: 0
    }

        .owl-theme .owl-dots button {
            width: 50px
        }
</style>

<!-- back button -->
<div class="flex items-center  border-b border-b-[#E8E8E8] sticky top-0 bg-white z-[10] relative px-4 py-[10px] h-[44px]" onclick="history.back()">
    <div class="flex gap-2 items-center absolute top-[12px] text-sm">
        <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" viewBox="0 0 22 22" fill="none">
            <path d="M15.2347 2.74084C14.7855 2.29167 14.0614 2.29167 13.6122 2.74084L5.99469 10.3583C5.63719 10.7158 5.63719 11.2933 5.99469 11.6508L13.6122 19.2683C14.0614 19.7175 14.7855 19.7175 15.2347 19.2683C15.6839 18.8192 15.6839 18.095 15.2347 17.6458L8.59802 11L15.2439 4.35417C15.6839 3.91417 15.6839 3.18084 15.2347 2.74084Z"
                  fill="#BCB08A" />
        </svg>@CRS.CUSTOMER.APPLICATION.Resources.Resource.Back
    </div>
    <div class="text-center w-full font-bold"></div>

</div>
<!-- breadcrumb  -->
<div class="flex py-[8px] px-4 gap-1 items-center breadcrumb">
    <a href="/" class="active" style="max-width:90px; overflow:hidden;text-overflow:ellipsis;white-space:nowrap">ホームページ</a>
    <i class="las la-angle-right"></i>
    <a onclick="RedirectToLocation();" class="active" style="max-width:90px; overflow:hidden;text-overflow:ellipsis;white-space:nowrap">店舗/ホスト一覧</a>
    <i class="las la-angle-right"></i>
    <a onclick="ClubDetailRedirectFunction('@ClubCode', '@LocationURL');" class="active" style="max-width:90px; overflow:hidden;text-overflow:ellipsis;white-space:nowrap">@Model.ClubNameJapanese</a>
    <i class="las la-angle-right"></i>
    <a href="#" style="max-width:90px; overflow:hidden;text-overflow:ellipsis;white-space:nowrap">@Model.HostNameJapanese</a>
</div>
<!-- host image  -->

<div class="relative">
    <div class="  ">
        <div class=" owl-carousel owl-theme no-scrollbar ">
            @if (Model.HostGalleryImageList != null && Model.HostGalleryImageList.Count > 0)
            {
                foreach (var item in Model.HostGalleryImageList)
                {
                    <img class="rounded object-cover h-[413px]" src="@item"
                         alt="modelimage">
                }
            }
            else
            {
                <div class="flex h-[413px] w-[294px] rounded ">
                    <img class="rounded object-cover" src="~/Content/assets/images/customer/demo-image.jpeg"
                         alt="modelimage" style="width:100%">
                </div>
            }
        </div>
    </div>
    <!-- favourite icon -->
    @if (!string.IsNullOrEmpty(Model.IsBookmarked) && Model.IsBookmarked.ToUpper() == "N")
    {
        <div class="absolute  right-3" style="bottom: -1.75rem; z-index:5" onclick="ManageBookmark('@Model.ClubId','@Model.HostId','host', this)">
            <img src="~/Content/assets/images/customer/favorite-unfilled.svg" alt="unfilled" style="height: 59px; width: 60px; z-index: 1" />
        </div>
    }
    else
    {
        <div class="absolute  right-3" style="bottom: -1.75rem;z-index:5" onclick="ManageBookmark('@Model.ClubId','@Model.HostId','host', this)">
            <img src="~/Content/assets/images/customer/favorite-filled.svg" alt="filled" style="height: 59px; width: 60px; z-index: 1 " />
        </div>
    }
</div>
