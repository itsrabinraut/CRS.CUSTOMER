                    style="background:linear-gradient(96deg, #99AEFB 10%, #F04AA4 61.32%, #E7B282 98.78%); border-radius:3px; color:#EDF1F6">
                <span class="py-3 text-center text-[15px] font-semibold leading-[23px] "
                      style="letter-spacing: -0.16px;">あなたとのマッチ度</span>
                <span class="py-3 text-[20px] font-bold leading-[23px]" style="letter-spacing: 1px;">@Model.CustomerHostCompatibility</span>
            </button>*@
    </div>

    <!-- introduction  -->
    <div class="mt-6 bg-[#EFEFEF]  px-4 " style="padding-top: 30px; padding-bottom:35px">
        <div class="flex justify-start items-center gap-[9px] ">
            <div class="">
                <svg xmlns="http://www.w3.org/2000/svg" width="4" height="43" viewBox="0 0 4 43" fill="none">
                    <path d="M0 43L0.8 43L0.799998 -3.49691e-08L-1.87959e-06 0L0 43ZM3.2 43L4 43L4 -1.74846e-07L3.2 -1.39877e-07L3.2 43Z"
                          fill="#BCB08A" />
                </svg>
            </div>
            <div>
                <div class="font-bold text-[21px] leading-[29.4px]" style="font-size:20px">Introduction</div>
                <div class="text-[#404040] leading-[16.8px] font-bold text-[12px] pb-[5px]">自己紹介</div>
            </div>
        </div>
        <div class="w-full h-[1px] bg-[#FDFBFA]"></div>
        @if (!string.IsNullOrEmpty(Model.HostIntroduction))
        {
            <div class="text-[#404040] pt-[5px] text-[12px] font-normal leading-[16.8px]" style="white-space:normal; text-overflow:ellipsis; overflow:hidden">
                @Model.HostIntroduction
            </div>
        }
        else
        {
            <div class="text-[#D17979] pt-[5px] text-[12px] font-normal leading-[16.8px]">
                未回答
            </div>
        }
    </div>
    <!-- host skills -->
    <div class="mt-6 px-4 mb-6">
        <div class="flex justify-start items-center gap-[9px] mb-[14px]">
            <div>
                <svg xmlns="http://www.w3.org/2000/svg" width="4" height="43" viewBox="0 0 4 43" fill="none">
                    <path d="M0 43L0.8 43L0.799998 -3.49691e-08L-1.87959e-06 0L0 43ZM3.2 43L4 43L4 -1.74846e-07L3.2 -1.39877e-07L3.2 43Z"
                          fill="#BCB08A" />
                </svg>
            </div>
            <div class="flex flex-col justify-start">
                <span class="text-[#292F33] text-[21px] font-bold leading-[29.4px]">Host Skills</span>
                <span class="text-[12px] font-bold leading-[16.8px] text-[#404040]">スキル自己評価</span>
            </div>

        </div>
        <div class="h-[1px] bg-[#EFEFEF] mb-[17px]"></div>
        <div class=" py-1">
            <canvas id="hostSkillChart" width="400" height="400"></canvas>
        </div>
    </div>

    <!-- personality -->
    <div class=" px-4">
        <div class="flex justify-start items-center gap-[9px] mb-[14px]">
            <div>
                <svg xmlns="http://www.w3.org/2000/svg" width="4" height="43" viewBox="0 0 4 43" fill="none">
                    <path d="M0 43L0.8 43L0.799998 -3.49691e-08L-1.87959e-06 0L0 43ZM3.2 43L4 43L4 -1.74846e-07L3.2 -1.39877e-07L3.2 43Z"
                          fill="#BCB08A" />
                </svg>
            </div>
            <div class="flex flex-col justify-start">
                <span class="text-[#292F33] text-[21px] font-bold leading-[29.4px]">Personality</span>
                <span class="text-[12px] font-bold leading-[16.8px] text-[#404040]">人物像</span>
            </div>

        </div>
        <div class="h-[1px] bg-[#EFEFEF] mb-[17px]"></div>
        <div>
            <div class="grid mt-[10px] mx-[16px] mb-[10px] grid-rows-9">
                <div style="line-height: 180%;" class="text-[#292F33] font-bold text-[12px]">
                    @{
                        var Personality = Model.HostIdentityDetailsModel.Where(x => x.LabelType.Trim().ToUpper() == "PERSONALITY").ToList();
                        foreach (var item in Personality)
                        {
                            <p style="line-height: 180%;">
                                @item.Label :
                                @if (!string.IsNullOrEmpty(item.LabelValue))
                                {
                                    <span class="text-[#404040] font-medium tracking-[1.2px] pl-1">
                                        @item.LabelValue
                                    </span>
                                }
                                else
                                {
                                    <span class="text-[#D17979] font-medium tracking-[1.2px] pl-1">
                                        未回答
                                    </span>
                                }
                            </p>
                        }
                    }
                </div>
            </div>
        </div>
    </div>


    <!-- Life Style -->
    <div class="my-6 px-[19px]" style="padding-left: 19px; padding-right: 19px;">
        <div class="flex justify-start items-center gap-[9px] mb-[14px]">
            <div>
                <svg xmlns="http://www.w3.org/2000/svg" width="4" height="43" viewBox="0 0 4 43" fill="none">
                    <path d="M0 43L0.8 43L0.799998 -3.49691e-08L-1.87959e-06 0L0 43ZM3.2 43L4 43L4 -1.74846e-07L3.2 -1.39877e-07L3.2 43Z"
                          fill="#BCB08A" />
                </svg>
            </div>
            <div class="flex flex-col justify-start">
                <span class="text-[#292F33] text-[21px] font-bold leading-[29.4px]">Lifestyle</span>
                <span class="text-[12px] font-bold leading-[16.8px] text-[#404040]">ライフスタイル</span>
            </div>

        </div>
        <div class="h-[1px] bg-[#EFEFEF] mb-[17px]"></div>
        @{
            var Lifestyle = Model.HostIdentityDetailsModel.Where(x => x.LabelType.Trim().ToUpper() == "LIFESTYLE").ToList();
            foreach (var item in Lifestyle)
            {
                <div class="mt-[10px] flex flex-col gap-[3px] text-[12px]">
                    <!-- question -->
                    <div class="font-bold whitespace-nowrap" style="line-height:180%; letter-spacing:1.2px ">@item.Label</div>
                    <!-- answer -->
                    @if (!string.IsNullOrEmpty(item.LabelValue))
                    {
                        <div class="text-ellipsis font-medium  text-[#404040]" style="line-height: 140%; letter-spacing:1.2px">
                            @item.LabelValue
                        </div>
                    }
                    else
                    {
                        <div class=" font-medium  text-[#D17979] text-ellipsis"
                             style="line-height: 140%; letter-spacing:1.2px; color:#D17979">未回答</div>
                    }
                </div>
            }
        }
    </div>

    <!-- club info -->
    <div class="mt-6 px-[12.5px] clubInfo flex justify-start items-center gap-2" onclick="ClubDetailRedirectFunction('@ClubCode', '@LocationURL');">
