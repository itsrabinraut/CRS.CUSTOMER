    <script>
        function highlightMention() {
            const mentions = document.querySelectorAll('.event-description.mentioned');
            mentions.forEach(element => {
                let text = element.innerHTML;
                const atIndex = text.indexOf('@@');
                if (atIndex !== -1) {
                    let partAfterAt = text.substring(atIndex);
                    let words = partAfterAt.split(/\s+/);
                    if (words.length >= 3 || words.length >= 2) {
                        let mentionText = words.slice(0, 3).join(' '); 
                        let restOfText = partAfterAt.substring(mentionText.length);

                        text = text.substring(0, atIndex) + '<span class="mentioned_name">' + mentionText + '</span>' + restOfText;
                        element.innerHTML = text;
                    }
                }
            });
        }
        highlightMention();
    </script>

