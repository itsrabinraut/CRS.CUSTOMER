@using CRS.CUSTOMER.APPLICATION.Library
@model CRS.CUSTOMER.APPLICATION.Models.LocationManagementV2.ClubDetailModel
@{
    var PrefecturesArea = ViewBag.LocationId;
    var LocationJapaneseLabel = ViewBag.LocationJapaneseLabel;
    var ClubId = ViewBag.ClubId;
    var ClubNameLabel = ViewBag.ClubName;
    string[] PrefecturesAreaPart = PrefecturesArea.Split('/');
    string[] PrefecturesAreaJapnesePart = LocationJapaneseLabel.Split('/');
    ViewBag.Title = $"{ClubNameLabel}‖{PrefecturesAreaJapnesePart[0]}({PrefecturesAreaJapnesePart[1]})のホストクラブ‖ホスログ《ホストクラブ初回来店・ネット予約》";
    Layout = "~/Views/Shared/_Layout.cshtml";
    DateTime todayDate = DateTime.Now;
    string formattedDate = todayDate.ToString("yyyy年M月");
    var target = ViewBag.target;
    var PlanGroupList = ViewBag.PlanGroup;
    DateTime currentTime = DateTime.Parse(DateTime.Now.ToString("HH:mm"));
}
<meta name="description" content="@ViewBag.MetaTagDescription">
<script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.8.1/flowbite.min.js"></script>
<script src="/Content/assets/js/tabs.js"></script>
<style>
    .stickyBox {
        position: -webkit-sticky;
        position: sticky;
        bottom: 0;
        height: 30px;
        width: 100vw;
        background: yellow;
    }
    .mentioned_name {
        color: #D17979;
    }
    .mentioned {
        font-size: 11px;
        font-weight: 500;
        color: #333333;
    }

    .tab-container-plane .active {
        border-bottom: 2px solid #E3A1A1;
    }

    .gallery {
        display: grid;
        grid-template-columns: repeat(3, 1fr);
        gap: 4px;
        max-width: 1200px;
        width: 100%;
        padding-top: 4px;
        padding-bottom: 4px;
        box-sizing: border-box;
    }

        .gallery img {
            width: 100%;
            aspect-ratio: 1;
            object-fit: cover; /* Ensures image covers the div while maintaining aspect ratio */
        }

    .galleryHost {
        display: grid;
        grid-template-columns: repeat(3, 1fr);
        gap: 14px;
        max-width: 1200px;
        width: 100%;
        padding-top: 4px;
        padding-bottom: 4px;
        box-sizing: border-box;
    }

        .galleryHost img {
            width: 100%;
            aspect-ratio: 1;
            min-height: 104px;
            min-width: 104px;
            object-fit: cover; /* Ensures image covers the div while maintaining aspect ratio */
        }
</style>
<!-- breadcrumb  -->
<div class="flex py-[8px] px-4 gap-1 items-center breadcrumb">
    <a href="/" class="active">ホームページ</a>
    <i class="las la-angle-right"></i>
    <a class="active" onclick="RedirectToLocation();">店舗/ホスト一覧</a>
    <i class="las la-angle-right"></i>
    <a href="#">@Model.ClubNameJp</a>
</div>
<div class="h-[189px]">
    <img src="@Model.ClubCoverPhoto" alt="banner" class="h-[189px] w-full object-cover"
         style="height: 189px;" />

</div>
<!-- section 1 -->

<div class="pt-[20px]">

    <ul class="club_tags">
        @if (!string.IsNullOrEmpty(Model.Tag1))
        {

            <li class="tag_location"><img class="map_pin" src="~/Content/assets/images/location_icon.svg" alt="ピン"> @Model.Tag1</li>
        }
        @if (!string.IsNullOrEmpty(Model.Tag2))
        {
            <li class="tag_rank">ランク @Model.Tag2</li>
        }
        @if (!string.IsNullOrEmpty(Model.Tag3))
        {
            <li class="tag_platinum">@Model.Tag3</li>
        }
        @if (!string.IsNullOrEmpty(Model.Tag4))
        {
            <li class="tag_good">@Model.Tag4</li>
        }
        @if (!string.IsNullOrEmpty(Model.Tag5))
        {
            <li class="tag_big">@Model.Tag5</li>
        }
    </ul>
</div>

<!-- section 2 -->
<div class="pl-4 pr-[2px] flex justify-between ">
    <div class="flex gap-[8px]">
        @if (Model.ClubLogo != null || Model.ClubLogo != "")
        {
            <div data-modal-target="" data-modal-toggle="">
                <img src="@Model.ClubLogo"
                     class="h-[48px] w-[48px] object-cover rounded-full border-[1px] border-[#FAFAFA]" />
            </div>
        }
        else
        {
            <div data-modal-target="" data-modal-toggle="">
                <img src="/Content/assets/images/logo.svg"
                     class="h-[48px] w-[48px] object-cover rounded-full border-[1px] border-[#FAFAFA]" />
            </div>
        }
        <div>
            <div class="club_info" style="width:auto;">
                <!-- s/club_name -->
                <h3 class="club_name">@Model.ClubNameEng</h3>
                <!-- e/club_name -->
                <!-- s/club kana -->
                <p class="club_kana">@Model.ClubNameJp</p>
                <!-- e/club kana -->
                <!-- s/club group -->
                <p class="club_group">@Model.GroupName</p>
                <!-- e/club group -->
            </div>
        </div>
    </div>
    @if (string.IsNullOrEmpty(Model.IsBookmarked) || Model.IsBookmarked.Trim().ToUpper() == "N")
    {
        <div class="flex items-center flex-col">
            <div class="p-1">
                <div class="action_keep" onclick="ManageBookmark('@Model.ClubId', '', 'club', this)"><img alt="保存する" src="~/Content/assets/images/bookmark-notfilled.svg"></div>
            </div>
        </div>
    }
    else
    {
        <div class="flex items-center flex-col">
            <div class="p-1">
                <div class="action_keep" onclick="ManageBookmark('@Model.ClubId', '', 'club', this)"><img alt="保存する" src="~/Content/assets/images/bookmark-filled.svg"></div>
            </div>
        </div>
    }

</div>
<div class="px-4 py-[12px]">
    <div class="club_rate" style="padding:0;">
        <!-- s/club rank -->
        <ul class="rate_stars">
            <!-- s/rank star -->
            @{
                var unratedScale = (5 - Model.AverageRating);
                for (int i = 0; i < Model.AverageRating; i++)
                {
                    <li><img src="~/Content/assets/images/ratingstar-filled.svg" alt="未評価"></li>
                }
                for (int i = 0; i < unratedScale; i++)
                {
                    <!-- e/rank star -->
                    <!-- s/rank star -->
                    <li><img src="~/Content/assets/images/ratingstar-notfilled.svg" alt="未評価"></li>
                }
            }
            <!-- e/rank star -->
            <!-- s/rank star -->
            @*<li><img src="/Content/assets/images/ratingstar-notfilled.svg" alt="未評価"></li>*@
            <!-- e/rank star -->
            <!-- s/rank star -->
            @* <li><img src="/Content/assets/images/ratingstar-notfilled.svg" alt="未評価"></li>*@
            <!-- e/rank star -->
            <!-- s/rank star -->
            @*<li><img src="/Content/assets/images/ratingstar-notfilled.svg" alt="未評価"></li>*@
            <!-- e/rank star -->
            <!-- s/rank star -->
            <li class="rate_value"> @Model.AverageRating</li>
            <!-- e/rank star -->
        </ul>
        <!-- e/club rank -->
        <!-- s/club review -->
        <ul class="rate_review">
            <!-- s/comment frame -->
            <li><img src="~/Content/assets/images/comment.svg" alt="コメント"></li>
            <!-- e/comment frame -->
            <!-- s/comment value -->
            <li class="comment_value">@Model.TotalComment</li>
            <!-- e/comment value -->
        </ul>
        <!-- e/club review -->
    </div>
    <div class="text-[12px] font-normal pt-2 leading-[18px] self-stretch	text-[#333]">
        @Model.ClubDescription
    </div>
</div>
<!-- calender -->
<div>
    <div class=" text-[10px] flex border-t border-b border-[#EFEFEF] justify-between px-[17px] border-custom">
        @foreach (var item2 in Model.ClubWeeklyScheduleList)
        {
            if (!string.IsNullOrEmpty(item2.EnglishDay) && item2.EnglishDay.ToUpper().Trim() == "SAT")
            {
                var isSatoff = item2.ScheduleLabel;
                if (!string.IsNullOrEmpty(isSatoff) && isSatoff.ToUpper().Trim() == "DAYOFF")
                {
                    <div style="background: #D9D9D9; opacity:0.3">
                        @item2.DayLabel
                    </div>

                }
                else
                {
                    <div class="text-blue">
                        @item2.DayLabel
                    </div>
                }

            }
            else if (!string.IsNullOrEmpty(item2.EnglishDay) && item2.EnglishDay.ToUpper().Trim() == "SUN")
            {
                var isSunoff = item2.ScheduleLabel;
                if (!string.IsNullOrEmpty(isSunoff) && isSunoff.ToUpper().Trim() == "DAYOFF")
                {
                    <div style="background: #D9D9D9; opacity:0.3">
                        @item2.DayLabel
                    </div>
                }
                else
                {
                    <div class="text-red">
                        @item2.DayLabel
                    </div>
                }
            }
            else if (!string.IsNullOrEmpty(item2.ScheduleLabel) && item2.ScheduleLabel.ToUpper().Trim() == "DAYOFF")
            {

                <div style="background: #D9D9D9; opacity:0.3">
                    @item2.DayLabel
                </div>
            }
            else
            {
                <div>
                    @item2.DayLabel
                </div>
            }
        }
    </div>
    <div class="text-[10px] flex border-t border-b border-[#EFEFEF] justify-between px-[17px] border-custom">
        @foreach (var item2 in Model.ClubWeeklyScheduleList)
        {
            var imageURL = "/Content/assets/images/" + item2.ScheduleLabel + ".svg";
            if (!string.IsNullOrEmpty(item2.Schedule) && item2.Schedule.Trim().ToUpper() == "RESERVABLE")
            {
                <div onclick="InitiateClubReservationFunction('@Model.ClubId','@item2.FullDate' )">
                    @item2.Date <div class="flex justify-center mt-1">
                        <img src="@imageURL" class="h-[11px] w-[12px]">
                    </div>
                </div>
            }
            else
            {
                <div style="background: #D9D9D9; opacity:0.3">
                    @item2.Date <div class="flex justify-center mt-1">
                        <img src="@imageURL" class="h-[11px] w-[11px]">
                    </div>
                </div>
            }
        }
    </div>
</div>
<div class="">
    <div class="flex px-[16px] gap-[24px]" style="border-bottom: 4px solid #EFEFEF;">
        <div class="flex tab-container-plane no-scrollbar bg-white gap-[14px] overflow-x-auto" style="overflow-y:hidden">
            <a id="tab1Button" class="tab-button-plane tab-link whitespace-nowrap @(target == "" ? "active" : "")" style="font-size:12px;" href="@($"/area{PrefecturesArea}/hostclub/{ViewBag.ClubId}/")">店舗トップ</a>
            <a id="tab2Button" class="tab-button-plane tab-link whitespace-nowrap @(target == "host" ? "active" : "")" style="font-size: 12px;" href="@($"/area{PrefecturesArea}/hostclub/{ViewBag.ClubId}/host")">ホスト</a>
            <a id="tab3Button" class="tab-button-plane tab-link whitespace-nowrap @(target == "review" ? "active" : "")" style="font-size: 12px;" href="@($"/area{PrefecturesArea}/hostclub/{ViewBag.ClubId}/review")">レビュー</a>
            <a id="tab4Button" class="tab-button-plane tab-link whitespace-nowrap @(target == "gallery" ? "active" : "")" style="font-size: 12px;" href="@($"/area{PrefecturesArea}/hostclub/{ViewBag.ClubId}/gallery")">写真</a>
            <a id="tab5Button" class="tab-button-plane tab-link whitespace-nowrap @(target == "schedule" ? "active" : "")" style="font-size: 12px;" href="@($"/area{PrefecturesArea}/hostclub/{ViewBag.ClubId}/schedule")">スケジュール</a>
            <a id="tab6Button" class="tab-button-plane tab-link whitespace-nowrap @(target == "notice" ? "active" : "")" style="font-size: 12px;" href="@($"/area{PrefecturesArea}/hostclub/{ViewBag.ClubId}/notice")">お知らせ</a>
        </div>
    </div>
</div>
