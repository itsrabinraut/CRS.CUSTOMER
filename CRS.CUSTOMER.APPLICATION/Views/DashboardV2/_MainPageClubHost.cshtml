                {
                    <div id="like" onclick="event.stopPropagation(); ManageBookmark('@item.ClubId','@item.HostId','host', this)">
                        <img src="~/Content/assets/images/loveicon-filled.svg" class="h-[30px] w-[30px]" alt="bookmark" />
