@model CRS.CUSTOMER.APPLICATION.Models.LocationManagement.LocationClubHostModel

<style>

    /* decorate */
    .disabled {
        color: #aaaaaa !important;
        background: #d7d7d7 !important;
    }

    .gradient {
        display: flex;
        justify-content: center;
        align-items: center;
        background: linear-gradient(96deg, #99aefb 10%, #f04aa4 61.32%, #e7b282 98.78%);
    }

    .list_wrap {
        width: 100%;
        height: auto;
        display: block;
        float: left;
        background: #efefef;
    }

    .club_content {
        width: 100%;
        height: auto;
        margin: 10px 0 0 0;
        padding: 15px 0 0 0;
        display: block;
        float: left;
        background: #fff;
    }

        .club_content:first-child {
            margin-top: 0;
        }

        .club_content:last-child {
            margin-bottom: 0;
        }

        .club_content a {
            width: 100%;
            display: block;
        }

    ul.club_tags, .club_header, .club_rate, .club_pr {
        padding-left: 15px;
        padding-right: 10px;
    }
    /* tags */
    ul.club_tags {
        width: 100%;
        height: auto;
        margin: 0 0 10px 0;
        display: inline-flex;
        flex-wrap: wrap;
    }

        ul.club_tags li {
            width: auto;
            height: auto;
            margin: 5px 0 0 5px;
            padding: 2px 8px;
            display: inline-flex;
            align-items: center;
            color: #ffffff;
            font-size: 10px;
            font-weight: 600;
            border-radius: 3px;
        }

            ul.club_tags li:first-child {
                margin-left: 0;
            }

            ul.club_tags li.tag_location {
                padding: 0;
                color: #aaaaaa;
                font-size: 0.8rem;
            }

            ul.club_tags li.tag_rank {
                background: #BFA3B6;
            }

            ul.club_tags li.tag_gold {
                background: #d4b78a;
            }

            ul.club_tags li.tag_good {
                background: #96b41f;
            }

            ul.club_tags li.tag_platinum {
                background: #697DBF;
            }

            ul.club_tags li.tag_big {
                background: #efb726;
            }
    /* hostclub info */
    .club_header {
        width: 100%;
        height: auto;
        display: flex;
        justify-content: space-between;
    }

    .club_logo {
        width: 45px;
        height: 45px;
        margin: 0 10px 0 0;
        padding: 0;
        display: inline-flex;
    }

        .club_logo img {
            width: 100%;
            height: 100%;
            border: 1px solid #e5e7eb;
            border-radius: 50%;
            background: #efefef;
            object-fit: cover;
        }

    .club_info {
        width: calc(100% - 100px);
        height: auto;
        max-height: 45px; /* as 「.club_logo」height */
        display: flex;
        flex-wrap: wrap;
        align-items: center;
    }

    h3.club_name, p.club_kana, p.club_group {
        display: inline-flex;
        align-items: center;
    }

    .club_info h3, .club_info p {
        width: 100%;
        font-size: 0.7rem;
        font-weight: 600;
    }

    h3.club_name {
        height: 19px;
        font-size: 0.9rem;
    }

    p.club_kana, p.club_group {
        height: 13px;
        color: #aaaaaa;
    }

    p.club_group {
        color: #e3a1a1;
        font-weight: normal;
    }

    .action_keep {
        width: 33px;
        height: 33px;
        padding: 1px;
        display: inline-flex;
        flex-flow: wrap-reverse;
    }

        .action_keep img {
            width: 33px;
            height: 33PX;
            aspect-ratio: 1 / 1;
        }
    /* hostclub thumb */
    ul.club_thumb {
        width: 100%;
        height: auto;
        margin: 10px 0;
        display: flex;
        flex-wrap: nowrap;
    }

        ul.club_thumb li {
            width: calc(100% / 3);
            height: auto;
            margin: 0;
            padding: 0 0 0 2px;
            display: inline-flex;
            aspect-ratio: 1 / 1;
        }

            ul.club_thumb li:first-child {
                padding: 0;
            }

            ul.club_thumb li img {
                width: 100%;
                height: 100%;
                aspect-ratio: 1 / 1;
                object-fit: cover;
                background: #efefef;
            }
    /* rate */
    .club_rate, ul.rate_stars, ul.rate_stars li, ul.rate_review {
        height: 18px; /* as 「ul.rate_stars li」 width */
    }

    .club_rate {
        width: 100%;
        height: 18px;
        display: flex;
        flex-wrap: nowrap;
        font-size: 1.1rem;
    }

    ul.rate_stars {
        display: flex;
        justify-content: flex-start;
        align-items: center;
    }

        ul.rate_stars li {
            width: 18px;
            margin: 0 0 0 2px;
            padding: 0;
            display: inline-flex;
            align-items: center;
        }

            ul.rate_stars li:first-child {
                margin: 0;
            }

            ul.rate_stars li img {
                width: 100%;
                height: 100%;
                object-fit: cover;
                aspect-ratio: 1 / 1;
            }

            ul.rate_stars li.rate_value {
                width: auto;
                height: 18px;
                margin: 0 0 0 8px;
                padding: 0;
                font-size: 20px;
                font-weight: bold;
                letter-spacing: 0;
            }

    ul.rate_review {
        width: auto;
        margin: 0 0 0 10px;
        display: inline-flex;
    }

        ul.rate_review li.comment_value {
            width: auto;
            height: 20px;
            margin: 0 0 0 2px;
            padding: 0;
            display: inline-flex;
            align-items: center;
            font-size: 14px;
        }

        ul.rate_review li:first-child {
            width: 20px;
            height: 20px;
        }

    .rate_review img {
        width: 100%;
        height: 100%;
        aspect-ratio: 1 / 1;
        object-fit: cover;
    }

    p.club_pr {
        width: 100%;
        height: 50px;
        display: inline-flex;
        flex-wrap: wrap;
        align-items: center;
        font-size: 0.75rem;
        overflow: hidden;
        line-height: 1.4;
        text-overflow: ellipsis;
    }
    /* hostclub footer */
    .club_footer {
        width: 100%;
        height: auto;
        margin: 10px 0 0 0;
        padding: 15px 15px 20px 15px;
        display: flex;
        justify-content: space-between;
        border-top: 1px solid #e5e7eb;
    }

    .club_status {
        width: 120px;
        height: auto;
        margin: 0;
        display: inline-flex;
        flex-flow: column;
    }

    .opening_hours {
        width: 140px;
        height: 18px;
        display: inline-flex;
        align-items: center;
        font-size: 0.85rem;
        font-weight: 600;
        letter-spacing: 0.02rem;
    }

        .opening_hours img {
            width: 18px;
            height: 18px;
            margin: 0 5px 0 0;
            display: inline-flex;
            align-items: center;
        }

    .available {
        width: 100%;
        height: auto;
        margin: 5px 0 0 0;
        padding: 1.4px;
        display: inline-flex;
        justify-content: center;
        align-items: center;
        color: #ffffff;
        font-size: 0.8rem;
        font-weight: 600;
        border-radius: 3px;
        background: #d4b78a;
    }

    .club_footer .rsv_btn {
        width: calc(100% - 135px);
        height: 45px;
        color: #ffffff;
        font-size: 1rem;
        font-weight: bold;
        display: inline-flex;
        letter-spacing: 1px;
        border-radius: 4px;
    }

    .book-now-btn {
        display: flex;
        justify-content: center;
        gap: 1px;
        width: 100%;
        flex-grow: 1;
    }

    .call-now-button {
        border-radius: 4px;
        border: 1px solid #D75A8B;
        display: flex !important;
        justify-content: center;
        gap: 4px;
        width: 100%;
        max-width: 84px;
        align-items: center;
        color: #D75A8B;
        font-size: 14px;
        font-weight: 600;
        letter-spacing: 0.56px;
        line-height: 150%;
        height: 45px;
    }

    .blocked-btn {
        border-radius: 4px;
        border: 1px solid #F1F1F1;
        background: #F0F0F0;
    }

    .list_wrap {
        background: #f8f8f8;
    }
</style>

